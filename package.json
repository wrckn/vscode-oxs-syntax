{
    "name": "vscode-oxs-syntax",
    "displayName": "OxyScript Language Support",
    "description": "Provides syntax highlighting for the OxyScript scripting language",
    "publisher": "wrckn",
    "version": "0.1.2",
    "repository": "https://github.com/wrckn/vscode-oxs-syntax",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "oxy-script",
            "aliases": ["OxyScript", "oxyscript", "oxy_script"],
            "extensions": [".oxs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "oxy-script",
            "scopeName": "source.oxs",
            "path": "./syntaxes/oxy-script.tmLanguage.json"
        }]
    }
}
